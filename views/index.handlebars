<!doctype html>

<html>
<head>
    <title>MoveSafe</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/css/MoveSafe.css" rel="stylesheet">
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div>
        <h1 class="header">Find Safe path</h1>
        <div class="centered">
            <a href="safePath"><button id="index-search-button" class="search-button">
                Search
            </button></a>
        </div>
        <p>Looking for these places?</p>
        <div class="centered">
            {{#each row1}}
                <a href="safePlace/{{place}}"><button id="{{id}}" class="places-button">
                    {{place}}
                </button></a>
            {{/each}}
        </div>
        <div class="centered">
            {{#each row2}}
                <a href="safePlace/{{place}}"><button id="{{id}}" class="places-button">
                    {{place}}
                </button></a>
            {{/each}}
        </div>
    </div>
<!-- <script src="https://code.jquery.com/jquery.js"></script> -->
<!-- <script src="/js/bootstrap.min.js"></script> -->
</body>
</html>