<body>



  <div class="topnav card-block">
      <div>
          <a id="navigationBarTitle" >Find a Safe Path</a>
      </div>
  </div>


  <div class="navBar card-block">
      <a href="/index" id="navigationBar hover">Home</a>
      <a href="javascript:history.back()" style="float: left; padding-left: 10%">Back</a>
      <a href="/" style="float: right; padding-right: 10%">Logout</a>
  </div>

<div align="center" class="cardboard safetyTips" style="position:relative;top:8vh;" >
        <div class="card-block" style="border: 1px solid lightgray;
    border-radius: 4px;" >
            <div class="card-header">
                <h1>Where do you want to go ?</h1>
            </div>
        </div>
</div>
<!--place holder--> 
<div class="cardboard search" style="position:relative;top:10vh" padding="10px">
  <form action="/safePath" method="post">
        <div class="centered" style="position:relative;top:2vh;margin-bottom: 20px">
            <img src="images/A_Point.png" >
            <input required id="origin-input-path" type="text" name="start" value="{{start}}" placeholder="enter a starting location...">
        </div>
        <div class="form-group centered" style="position:relative;top:0vh;margin-bottom: 20px">
            <img src="images/B_Point.png">
            <input required id="destination-input-path" type="text" name="destination" value="{{destination}}" placeholder="enter a destination...">
        </div>

        <div class="form-group" style="position:relative;left:60vw">
            <button id="searchButton" class="bouton btn-success" type="submit">Search</button>
        </div>
  </form>
</div>
</div>

<div align="center" class="cardboard safetyTips" style="position:relative;top:15vh;" >
        <div class="card-block"style="border: 1px solid lightgray;
    border-radius: 4px;">
            <div class="card-header" >
                <h1>Safety tip of the day</h1>
            </div>
        </div>
</div>

<div class="safetyTips" style="position:relative; top:15vh;left:5vw">
  <div style='background-color : white; border: 1px solid lightgray;
    border-radius: 4px;'>
      <p>{{tip}}</p>
  </div>
</div>


<script src="/js/MoveSafe.js"></script>
