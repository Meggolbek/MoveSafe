<body>
  <div class="topnav card-block">
      <div>
          <a style="position:absolute;left:25vw;" id="navigationBarTitle" >Find a Safe Path</a>
      </div>
  </div>
  <div class="navBar card-block" style="text-align: center">
      <a href="/index" id="navigationBar hover" style="color:white; align: center">Home</a>
      <a href="javascript:history.back()" style="color:white; float: left; padding-left: 10%">Back</a>
      <a href="/" style="color:white; float: right; padding-right: 10%">Logout</a>
  </div>

<!--place holder--> 
<div class="cardboard" style="position:relative;top:-1vh" padding="10px">
  <form action="/safePath" method="post">
    <div class="form-group centered" style="position:relative;top:2vh;margin-bottom: 20px">
        <img src="images/A_Point.png" height="35" width="27" style="margin:0 0 10px 0;">
        <input required id="origin-input-path" class="controls" type="text" name="start" value="{{start}}" placeholder="Enter a starting location">
    </div>
    <div class="form-group centered" style="position:relative;top:0vh;margin-bottom: 20px">
        <img src="images/B_Point.png" height="35" width="32" style="margin:0 0 10px 0;">
        <input required id="destination-input-path" class="controls"  type="text" name="destination" value="{{destination}}" placeholder="Enter a destination">
    </div>
    <div class="form-group centered" style="position:relative;top:0vh">
        <button class="btn btn-primary" style="height:40px; width:280px; font-size:20px; line-height:2px; font-family:Century Gothic; margin-left: 42px" type="submit">Search</button>
    </div>
  </form>
</div>
</div>
<br><br><br><br><br>
<div align="center" class="cardboard" style="position:relative;top:-1vh" padding="10px">
        <div class="card-block" style="max-width: 250px; width: 91vw";>
            <div class="card-header" style="font-family:Century Gothic; ">
                <h4>Safety tip of the day</h4>
            </div>
        </div>
</div>
  <div>
      <div style="width: 60vw;">
            <p style="font-family: Century Gothic; font-style: italic;font-size:8vw;position: relative;right:-20vw;top:3vh">{{tip}}</p>
        </div>
    </div>
  </div>
<script src="/js/MoveSafe.js"></script>
<!--<script src="/routes/SafePath.js"></script>-->
  </body>
</html>